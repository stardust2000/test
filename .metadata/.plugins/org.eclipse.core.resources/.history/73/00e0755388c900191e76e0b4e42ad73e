drop database if exists sqlkadaidb;
create database if not exists sqlkadaidb;
use sqlkadaidb;

create table userlist(
	id int not primary key auto_increment,
	user_name varchar(255),
	age int(16),
	address varchar(255)
	);


	insert into userlist(1,"山田",35,"東京都");
	insert into userlist(2,"佐藤",28,"神奈川県");
	insert into userlist(3,"門前",22,"千葉県");
	insert into userlist(4,"鈴木",31,"東京都");